{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/img/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/avif"
        }
      ]
    },
    {
      "source": "/public/favicon_io/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "rewrites": [
    {
      "source": "/favicon.ico",
      "destination": "/public/favicon_io/favicon.ico"
    },
    {
      "source": "/apple-touch-icon.png",
      "destination": "/public/favicon_io/apple-touch-icon.png"
    },
    {
      "source": "/favicon-32x32.png",
      "destination": "/public/favicon_io/favicon-32x32.png"
    },
    {
      "source": "/favicon-16x16.png",
      "destination": "/public/favicon_io/favicon-16x16.png"
    }
  ]
}
